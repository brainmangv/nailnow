        <splash density="xhdpi" height="960" orientation="portrait" src="images/720x960.png" width="720" />
        <feature name="SocialSharing">
            <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
        </feature>
    </platform>
    <platform name="ios">
