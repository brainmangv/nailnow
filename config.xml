<?xml version='1.0' encoding='utf-8'?>
<widget id="co.nailnow.app" version="0.1.25" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.01">
    <name>Nailnow</name>
    <description>
        Manicure a qualquer hora.
    </description>
    <author email="dev@nailnow.co" href="http://nailnow.co">
        Ricardo Ramos
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <access origin="cdvfile://*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="google.streetview:*" />
    <allow-intent href="google.navigation:*" />
    <allow-intent href="cdvfile://*" />
    <preference name="phonegap-version" value="cli-6.1.0" />
    <preference name="android-targetSdkVersion" value="16" />
    <preference name="android-installLocation" value="auto" />
    <preference name="orientation" value="portrait" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <icon src="icon.png" />
    <splash src="images/720x960.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" height="36" src="images/36x36.png" width="36" />
        <icon density="mdpi" height="48" src="images/48x48.png" width="48" />
        <icon density="hdpi" height="72" src="images/72x72.png" width="72" />
        <icon density="xhdpi" height="96" src="images/96x96.png" width="96" />
